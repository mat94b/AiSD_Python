import random

class RandomQueue:
	def __init__(self):
		self.maximum=20
		self.minimum = 0
		self.listOfElements = list()
		self.n = 0

	def is_empty(self):
		if not self.listOfElements:
			return True
		else:
			return False

	def is_full(self):
		if self.listOfElements is self.maximum:
			return True
		else:
			return False

	def insert(self, item):
		if(self.maximum == self.n):
			raise IndexError
		self.n+=1
		self.listOfElements.append(item)

	def remove(self):
		if not self.listOfElements:
			raise ValueError
		self.n -= 1
		helper = random.randint(self.minimum, self.n)
		result = self.listOfElements[helper]
		self.listOfElements.remove(result)
		return result


kolejka = RandomQueue()
kolejka.insert(5)
kolejka.insert(3)
kolejka.insert(4)
kolejka.insert(10)
kolejka.insert(14)
kolejka.insert(2)
for i in kolejka.listOfElements:
    print str(i)
while(kolejka.listOfElements!=[]):
    print "Usuwam!!!!"
    kolejka.remove()
    for i in kolejka.listOfElements:
        print str(i)
